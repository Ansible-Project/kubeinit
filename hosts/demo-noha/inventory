[hypervisor-nodes]
hypervisor-01 ansible_host=192.168.1.20 ansible_ssh_common_args='-o StrictHostKeyChecking="no"'

[kubernetes-master-nodes]
kubernetes-master-01 ansible_host=10.0.0.1 ansible_ssh_common_args='-o StrictHostKeyChecking="no" -o ProxyCommand="ssh -W %h:%p root@192.168.1.20"'

[kubernetes-worker-nodes]
kubernetes-worker-01 ansible_host=10.0.0.4 ansible_ssh_common_args='-o StrictHostKeyChecking="no" -o ProxyCommand="ssh -W %h:%p root@192.168.1.20"'
kubernetes-worker-02 ansible_host=10.0.0.5 ansible_ssh_common_args='-o StrictHostKeyChecking="no" -o ProxyCommand="ssh -W %h:%p root@192.168.1.20"'
kubernetes-worker-03 ansible_host=10.0.0.6 ansible_ssh_common_args='-o StrictHostKeyChecking="no" -o ProxyCommand="ssh -W %h:%p root@192.168.1.20"'
